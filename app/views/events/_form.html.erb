<%= form_for @event, :html => { :multipart => true } do |f| %>
    Title:<br><%= f.text_field :title, size: "60"  %><br>
    Event Date:<br> <%= f.text_field :date_of_event, :placeholder => "YYYY-MM-DD", size: "30"  %><br>
    Frequency:<br> <%= f.text_field :frequency, size: "30"  %><br>
    Description:<br> <%= f.text_field :description, size: "60"  %><br>
    Link to Photos:<br> <%= f.text_field :album_link , size: "60" %><br><br>
    
  <%= f.submit %><br><br>
<% end %>
