<%= form_for @medium, :html => { :multipart => true } do |f| %>
    Title:<br><%= f.text_field :title, size: "60"  %><br>
    Website Address:<br><%= f.text_field :link, size: "60"  %><br>
    Category: <br><%= f.select :category, options_for_select(["article", "photo", "video"])%><br>
  <%= f.submit %>
<% end %>